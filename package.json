{
  "name": "unit-testing-js",
  "version": "1.0.1",
  "description": "测试 工具包 ",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "unpkg": "/lib/",
  "jsdelivr": "/lib/",
  "types": "lib/index.d.ts",
  "scripts": {
    "commitlint": "commitlint -e",
    "prepare": "husky install",
    "build": "npx rollup -c --extend rollup.config.ts",
    "test": "ts-node-dev --respawn --transpile-only ./test/index.ts",
    "dev": "ts-node-dev --respawn --transpile-only ./test/index.test.ts",
    "md": "ts-node-dev --respawn --transpile-only ./md.ts",
    "restart": "rimraf dist && npm run build && npm start",
    "start": "node ./dist/index.js",
    "prod": "npm run build && npm run start",
    "eslint": "eslint . --ext .ts --fix",
    "pub:build": "npm run build && npm publish",
    "pub": "npm publish"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:abandonjs/unit-testing-js.git"
  },
  "bugs": {
    "url": "https://github.com/abandonjs/unit-testing-js/issues"
  },
  "keywords": [
    "test",
    "unit",
    "typescript",
    "javascript"
  ],
  "engines": {
    "node": ">=16"
  },
  "author": "ruihuag",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/config-conventional": "^17.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@types/bluebird": "^3.5.36",
    "@types/node": "^16.11.21",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "abandonjs": "^3.1.1",
    "asura-eye": "^0.6.1",
    "commitlint": "^17.3.0",
    "eslint": "^8.7.0",
    "fakingjs": "^0.1.1",
    "husky": "^8.0.2",
    "lint-staged": "^12.3.2",
    "node-notifier": "^8.0.2",
    "prettier": "^2.5.1",
    "reflect-metadata": "^0.1.13",
    "rh-color": "1.0.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.66.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "ts-toolbelt": "^9.6.0",
    "typescript": "^4.5.5"
  }
}
